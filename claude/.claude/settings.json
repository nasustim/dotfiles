{
  "permissions": {
    "allow": [
      "Bash(npm run lint)",
      "Bash(npm run test:*)",
      "Bash(git:*)",
      "Read(./src/**)",
      "Edit(./src/**)"
    ],
    "deny": [
      "Bash(curl:*)",
      "Bash(npm publish:*)",
      "WebFetch(domain:*)",
      "Read(.env.*)"
    ]
  },
  "includeCoAuthoredBy": false,
  "env": {}
}
