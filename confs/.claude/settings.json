{
  "env": {
    "MAX_THINKING_TOKENS": "10000"
  },
  "includeCoAuthoredBy": true,
  "permissions": {
    "allow": [
      "Bash",
      "Read",
      "WebFetch",
      "WebSearch"
    ],
    "ask": [
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(rm:*)",
      "Bash(git rm:*)",
      "Bash(git reset:*)"
    ],
    "deny": [
      "Bash(cat:*)",
      "Bash(npm publish:*)",
      "Bash(pnpm publish:*)",
      "Bash(bun publish:*)",
      "Bash(git push:*)",
      "Bash(git rebase:*)",
      "Read(.env)",
      "Read(.env.*)",
      "Read(**/secrets/**)",
      "Read(.dev.var)"
    ],
    "defaultMode": "bypassPermissions"
  },
  "enabledPlugins": {
    "typescript-lsp@claude-plugins-official": true
  }
}